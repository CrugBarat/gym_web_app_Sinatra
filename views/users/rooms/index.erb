<section>
  <div class="grid">
    <div class="grid-row grid-row-sm">
      <div class="cell_container">
        <a href="/users">
          <div class="cell1"><img class="img1" src="/images/flex3.webp" alt="">
            <div class="bottom-right">MAIN MENU</div>
          </div>
        </a>
      </div>
      <div class="cell_container">
        <a href="/users/bookings/new">
          <div class="cell2"><img class="img1" src="/images/flex1.webp" alt="">
            <div class="bottom-right">BOOK</div>
          </div>
        </a>
      </div>
    </div>
  </section>
  <section>
    <% for room in @rooms %>
    <div class="index">
      <br>
      <img class="rooms" src="<%=room.image()%>" alt="">
      <p>Studio: <%=room.name()%></p>
      <a href="/users/rooms/<%= room.id %>/equipment">
        <button type="button" name="button">EQUIPMENT</button>
      </a>
      <br>
    </div>
    <br>
    <%end%>
    <br/>
    <br/>
  </section>
  <section>
    <div class="quote">
      <p>“If you don’t find the time,</p>
    </div>
    <div class="quote-middle">
      <p>&nbsp; if you don’t do the work,</p>
    </div>
    <div class="quote">
      <p>&nbsp; you don’t get the results.”</p>
    </div>
    <p class="quote-name"> – Arnold Schwarzenegger</p>
  </section>
  <section>
    <div class="grid-vid">
      <video class="video-insert" controls autoplay>
        <source src="/videos/vid1.mp4" type="video/mp4">
        </video>
      </div>
    </section>
