<section>
  <div class="grid">
    <div class="grid-row grid-row-sm">
      <div class="cell_container">
        <a href="/users">
          <div class="cell1"><img class="img1" src="/images/flex1.webp" alt="">
            <div class="bottom-right">MAIN MENU</div>
          </div>
        </a>
      </div>
      <div class="cell_container">
        <a href="/users/sessions">
          <div class="cell3"><img class="img1" src="/images/flex3.webp" alt="">
            <div class="bottom-right">SESSIONS</div>
          </div>
        </a>
      </div>
    </div>
  </section>
  <section>
    <div class="index">
      <br>
      <p>Member: <%= @member.full_name() %></p>
      <p>DOB: <%=@member.formatted_dob()%></p>
      <p>Membership Type: <%=@member.membership_type()%></p>
      <p>Address: <%= @member_details.address() %></p>
      <p>Postcode: <%= @member_details.post_code() %></p>
      <p>Telephone: <%= @member_details.formatted_telephone() %></p>
      <p>Email: <%= @member_details.email_address() %></p>
      <p><%=@member.show_user_active()%></p>
      <br>
    </div>
    <br/>
    <br/>
  </section>
  <section>
    <div class="quote">
      <p>“If you don’t find the time,</p>
    </div>
    <div class="quote-middle">
      <p>&nbsp; if you don’t do the work,</p>
    </div>
    <div class="quote">
      <p>&nbsp; you don’t get the results.”</p>
    </div>
    <p class="quote-name"> – Arnold Schwarzenegger</p>
  </section>
  <section>
    <div class="grid-vid">
      <video class="video-insert" autoplay loop>
        <source src="/videos/vid4.mp4" type="video/mp4">
        </video>
      </div>
    </section>
