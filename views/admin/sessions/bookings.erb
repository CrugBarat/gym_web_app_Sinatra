<div class="home-container">
  <img class="home" alt="">
  <div class="bottom-left">Admin.</div>
</div>
<section>
  <div class="grid">
    <div class="grid-row grid-row-sm">
      <div class="cell_container">
        <a href="/admin">
          <div class="cell1"><img class="img1" src="/images/flex3.webp" alt="">
            <div class="bottom-right">MAIN MENU</div>
          </div>
        </a>
      </div>
      <div class="cell_container">
        <a href="/admin/sessions">
          <div class="cell2"><img class="img1" src="/images/flex6.webp" alt="">
            <div class="bottom-right">SESSIONS</div>
          </div>
        </a>
      </div>
      <div class="cell_container">
        <a href="/admin/sessions/create">
          <div class="cell3"><img class="img1" src="/images/flex1.webp" alt="">
            <div class="bottom-right">CREATE</div>
          </div>
        </a>
      </div>
    </div>
  </section>
  <section>
    <% for member in @members %>
    <div class="index">
      <br>
      <p style="color:#fe5ebb;">Member: <span style="color:#414141;"><%=member.full_name()%></span></p>
      <p style="color:#fe5ebb;">Membership No: <span style="color:#414141;"><%=member.id()%></span></p>
      <% booking = Booking.find_by_member_id(member.id) %>
      <a href="/admin/bookings/<%= booking.id %>/edit">
        <button type="button" name="button">EDIT BOOKING</button>
      </a>
    </div>
    <br>
    <%end%>
    <br/>
    <br/>
  </section>
