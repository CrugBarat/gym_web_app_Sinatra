<section>
  <div class="grid">
    <div class="grid-row grid-row-sm">
      <div class="cell_container">
        <a href="/admin">
          <div class="cell1"><img class="img1" src="/images/flex6.webp" alt="">
            <div class="bottom-right">MAIN MENU</div>
          </div>
        </a>
      </div>
      <div class="cell_container">
        <a href="/admin/rooms">
          <div class="cell2"><img class="img1" src="/images/flex3.webp" alt="">
            <div class="bottom-right">STUDIOS</div>
          </div>
        </a>
      </div>
      <div class="cell_container">
        <a href="/admin/rooms/create">
          <div class="cell3"><img class="img1" src="/images/flex1.webp" alt="">
            <div class="bottom-right">CREATE</div>
          </div>
        </a>
      </div>
    </div>
  </section>
  <section>
    <% for room in @rooms %>
    <div class="index">
      <br>
      <p>Studio: <%=room.name()%></p>
      <p>Studio ID: <%=room.id()%></p>
      <a href="/admin/rooms/<%= room.id() %>/edit">
        <button type="button" name="button">EDIT</button>
      </a>
      <a href="/admin/rooms/<%= room.id() %>/equipment">
        <button type="button" name="button">EQUIPMENT</button>
      </a>
      <br>
    </div>
    <br>
    <%end%>
    <br/>
    <br/>
  </section>
