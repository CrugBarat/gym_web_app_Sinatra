<section>
  <div class="grid">
    <div class="grid-row grid-row-sm">
      <div class="cell_container">
        <a href="/admin/members">
          <div class="cell1"><img class="img1" src="/images/sessions.webp" alt="">
            <div class="bottom-right">MEMBERS</div>
          </div>
        </a>
      </div>
      <div class="cell_container">
        <a href="/admin/sessions">
          <div class="cell2"><img class="img1" src="/images/sessions.webp" alt="">
            <div class="bottom-right">SESSIONS</div>
          </div>
        </a>
      </div>
      <div class="cell_container">
        <a href="/admin/bookings">
          <div class="cell3"><img class="img1" src="/images/sessions.webp" alt="">
            <div class="bottom-right">BOOKINGS</div>
          </div>
        </a>
      </div>
    </div>
  </section>
  <section>
    <% for room in @rooms %>
    <div class="index">
      <p>Room: <%=room.name()%></p>
      <p>Room ID:<%=room.id()%></p>
      <a href="/admin/rooms/<%= room.id() %>/edit">
        <button type="button" name="button">EDIT</button>
      </a>
      <a href="/admin/rooms/<%= room.id() %>/equipment">
        <button type="button" name="button">EQUIPMENT</button>
      </a>
    </div>
    <%end%>
  </section>
