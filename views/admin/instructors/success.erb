<div class="home-container">
  <img class="home" alt="">
  <div class="bottom-left">Admin.</div>
</div>
<section>
  <div class="grid">
    <div class="grid-row grid-row-sm">
      <div class="cell_container">
        <a href="/admin">
          <div class="cell1"><img class="img1" src="/images/flex6.webp" alt="">
            <div class="bottom-right">MAIN MENU</div>
          </div>
        </a>
      </div>
      <div class="cell_container">
        <a href="/admin/instructors">
          <div class="cell2"><img class="img1" src="/images/flex1.webp" alt="">
            <div class="bottom-right">INSTRUCTORS</div>
          </div>
        </a>
      </div>
      <div class="cell_container">
        <a href="/admin/instructors/create">
          <div class="cell3"><img class="img1" src="/images/flex3.webp" alt="">
            <div class="bottom-right">CREATE</div>
          </div>
        </a>
      </div>
    </div>
  </section>
  <section>
    <div class="index">
      <br>
      <p style="color:#fe5ebb;">Instructor: <span style="color:#414141;"><%= @new_instructor.full_name() %></span></p>
      <p style="color:#fe5ebb;">Profile: <span style="color:#414141;"><%= @new_instructor.profile() %></span></p>
      <p style="color:#fe5ebb;">Instructor ID: <span style="color:#414141;"><%= @new_instructor.id() %></span></p>
      <p style="color:#fe5ebb;">Email: <span style="color:#414141;"><%= @new_instructor.email_address() %></span></p>
      <a href="/admin/instructors/<%= @new_instructor.id() %>/edit">
        <button type="button" name="button">EDIT</button>
      </a>
      <br>
    </div>
    <br/>
    <br/>
  </section>
