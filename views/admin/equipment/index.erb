<a href="/admin/equipment/create">
  <button type="button" name="button">CREATE</button>
</a>
<% for equipment in @equipment %>
<div class="index">
  <p>Type: <%=equipment.type()%></p>
  <p>Quantity: <%=equipment.quantity()%></p>
  <p>Room: <%=equipment.show_room_name()%></p>
  <a href="/admin/equipment/<%= equipment.id() %>/edit">
    <button type="button" name="button">EDIT</button>
  </a>
</div>
<%end%>
